<Window x:Class="Minibrowser.UrlOeffnenDialog"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:Minibrowser"
        mc:Ignorable="d"
        Title="Öffnen..." Height="187" Width="300" Topmost="True" WindowStyle="ToolWindow" Loaded="Window_Loaded" ResizeMode="NoResize">
    <Grid>
        <Label x:Name="label" Content="Öffnen" HorizontalAlignment="Left" Margin="10,46,0,0" VerticalAlignment="Top"/>
        <TextBox x:Name="textBox" HorizontalAlignment="Left" Height="23" Margin="91,50,0,0" TextWrapping="Wrap" VerticalAlignment="Top" Width="170" TextChanged="textBox_TextChanged"/>
        <Button x:Name="buttonOeffnen" Content="Öffnen" HorizontalAlignment="Left" Margin="10,108,0,0" VerticalAlignment="Top" Width="75" Click="buttonOeffnen_Click" IsDefault="True" IsEnabled="False"/>
        <Button x:Name="buttonAbbrechen" Content="Abbrechen" HorizontalAlignment="Left" Margin="186,108,0,0" VerticalAlignment="Top" Width="75" Click="buttonAbbrechen_Click" IsCancel="True"/>

    </Grid>
</Window>
